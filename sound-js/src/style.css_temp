
.vis-container {
    margin-top: 2rem;
    background: #000;
    border: 1px solid #333;
    border-radius: 8px;
    overflow: hidden;
    width: 100%;
    max-width: 800px;
    height: 500px;
    margin-left: auto;
    margin-right: auto;
}

#vis-canvas {
    width: 100%;
    height: 100%;
    display: block;
}
